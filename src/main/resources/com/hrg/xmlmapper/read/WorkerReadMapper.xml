        </foreach>
      </if>
      <if test="example.createtime != null" >
         and createtime = #{example.createtime,jdbcType=TIMESTAMP}
      </if>
    </where>
  </sql>
  <sql id="Base_Column_List" >
    dataid, name, phone, department, remark, account, password, departmentdataid, state, 
