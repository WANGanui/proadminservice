          #{listItem}
        </foreach>
      </if>
      <if test="example.password != null" >
         and password = #{example.password,jdbcType=VARCHAR}
      </if>
      <if test="example.passwordList != null  and  example.passwordList.size() > 0 " >
         and password in 
